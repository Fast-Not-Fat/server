{
  "name": "fast-not-fat-server",
  "version": "1.0.0",
  "main": "src/server.ts",
  "repository": "https://github.com/Fast-Not-Fat/server",
  "author": "Fast Not Fat",
  "license": "MIT",
  "engines": {
    "node": "13.9.x"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "start": "node dist/server.js",
    "watch": "nodemon",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/socket.io": "^2.1.4",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fast-not-fat": "git+https://github.com/Fast-Not-Fat/types.git",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.2",
    "socket.io": "^2.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^1.19.1"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & npm update fast-not-fat && git add package-lock.json"
    }
  }
}
