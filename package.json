    "@types/socket.io": "^2.1.4",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.2",
    "socket.io": "^2.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
